{
  "name": "@walltowall/calico",
  "version": "0.1.0",
  "description": "React components for using atmoically generated styles via props.",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "sideEffects": false,
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "release": "standard-version"
  },
  "homepage": "https://github.com/walltowall/calico",
  "repository": {
    "type": "git",
    "url": "https://github.com/walltowall/calico.git"
  },
  "keywords": [
    "react",
    "components",
    "static",
    "atomic",
    "css",
    "treat"
  ],
  "dependencies": {
    "clsx": "^1.1.1",
    "fp-ts": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "csstype": "^2.6.10",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "treat": "^1.4.1",
    "ts-jest": "^26.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-treat": ">=1.4.0",
    "treat": ">=1.4.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
